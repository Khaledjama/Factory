@model IEnumerable<New_System.Models.orders>

@{
    ViewBag.Title = "All";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
@using (Html.BeginForm("SearchForOrders", "Orders"))
{
    <b><br /></b>
    <p>Get Order By Date</p>
    <label> From </label>
    <input type="date" name="date1" id="date" />
    <label> TO </label>
    <input type="date" name="date2" id="date" />
    <input type="submit" value="Search" />
}
<p>
    <br />
<p>
    @Html.ActionLink("All Bills", "All")
</p>

<br />
<table class="table">
    <tr>
        <th>
            Customer Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Adress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GetOrder_Date)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AspNetUsers.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GetOrder_Date)
            </td>
            <td>
                @Html.ActionLink("Details", "GetMyOrderDetails", new { id = item.Id }) |
            </td>
        </tr>
    }
</table>
@Html.ActionLink("Download_PDF", "Download_PDF");
